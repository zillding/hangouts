{
  "name": "hangouts",
  "version": "1.0.0",
  "description": "A web application allow people video call while watching YouTube",
  "repository": {
    "type": "git",
    "url": "https://github.com/zillding/hangouts.git"
  },
  "author": "Zill Ding",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run test && npm run build:clean",
    "build:clean": "rimraf ./build/*",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p",
    "analyze": "node ./internals/scripts/analyze.js",
    "start": "npm run start:tunnel",
    "start:tunnel": "cross-env NODE_ENV=development node server",
    "start:prod": "cross-env NODE_ENV=production node server",
    "pagespeed": "node ./internals/scripts/pagespeed.js",
    "presetup": "npm i chalk",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "npm run lint:eslint && npm run lint:jscs",
    "lint:js:staged": "npm run lint:eslint:staged && npm run lint:jscs:staged",
    "lint:eslint": "eslint .",
    "lint:eslint:staged": "eslint-staged",
    "lint:jscs": "jscs .",
    "lint:jscs:staged": "jscs-staged",
    "lint:css": "stylelint ./app/**/*.css",
    "lint:css:staged": "stylelint-staged",
    "lint:staged": "npm run lint:js:staged && npm run lint:css:staged",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test karma start internals/testing/karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls"
  },
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "chalk": "^2.4.2",
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "fontfaceobserver": "^2.1.0",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "material-ui": "^0.20.2",
    "qs": "^6.2.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-draggable": "^3.1.1",
    "react-flex": "^2.2.1",
    "react-github-fork-ribbon": "^0.6.0",
    "react-pure-render": "^1.0.2",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-redux": "^4.0.4",
    "react-router-scroll": "^0.4.4",
    "react-timeago": "^4.3.0",
    "react-youtube": "^7.9.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.1",
    "reselect": "^4.0.0",
    "simplewebrtc": "^3.0.2",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^4.2.0",
    "chai-enzyme": "^0.8.0",
    "cheerio": "^0.22.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^24.1.0",
    "expect-jsx": "^5.0.0",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "ip": "^1.1.2",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "jscs": "^3.0.7",
    "json-loader": "^0.5.4",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "lint-staged": "^8.1.4",
    "mocha": "^5.2.0",
    "ngrok": "3.1.1",
    "null-loader": "^0.1.1",
    "offline-plugin": "^5.0.6",
    "plop": "^2.2.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-focus": "^4.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-reporter": "^6.0.1",
    "psi": "^3.1.0",
    "rimraf": "^2.5.2",
    "shelljs": "^0.8.3",
    "sinon": "^7.2.3",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.3",
    "webpack-dev-middleware": "^3.5.2",
    "webpack-hot-middleware": "^2.10.0"
  }
}
